node {
        stage("preparation"){
                git "https://github.com/mahmoud254/simple-java-maven-app.git"
        }

        stage('Build') {

                sh 'echo hi'
                sh 'ls'

        }
        try{
        stage('Test') {

                sh 'echo testing'
        }
        }
        catch(e){
                echo "error"
        }
        finally{
                echo currentBuild.currentResult
                if (currentBuild.currentResult=="SUCCESS") {
                        echo "successful pipeline"
                }
        
        }
        stage('Deliver') {

                sh 'echo test branch'

        }
    
}
